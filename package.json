{
  "name": "pdf-json",
  "version": "0.0.1",
  "description": "Generates PDF documents using PDF-Make and a basic JSON templating system",
  "main": "index.js",
  "scripts": {
    "precompile": "rimraf --glob 'dist/lib'",
    "buildes6": "tsc",
    "buildes5": "tsc --build tsconfig.es5.json",
    "compile": "npm run lint && tsc && tsc --build tsconfig.es5.json",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dataclear/pdf-json.git"
  },
  "author": "Dataclear Ltd",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "coveralls": "^3.1.1",
    "ejs": "^3.1.9",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "js-htmlencode": "^0.3.0",
    "jsdoc-parse-plus": "^1.3.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "pdfmake": "^0.2.7"
  }
}
